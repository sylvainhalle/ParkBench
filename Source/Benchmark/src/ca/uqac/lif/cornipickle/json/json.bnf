# -----------------------------------------
# Grammar for JSON; do not edit!
# -----------------------------------------

<S>             := <list> | <object> | <string> | <number> | <boolean> | <null> ;

<list>          := [ <list_contents> ] ;
<list_contents> := <S> , <list_contents> | <S> | ε ;

<object>        := { <prop_list> } ;
<prop_list>     := <prop> , <prop_list> | <prop> | ε ;
<prop>          := <attribute> : <S>;
<attribute>     := <string>;

<string>        := ^"[^"]*";
<number>        := ^[\-\d\.]+;
<boolean>       := true | false ;
<null>          := null ;